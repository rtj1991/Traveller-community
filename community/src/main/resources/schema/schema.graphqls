type Mutation {
    createUser(user: CredentialsInputDto): User
    login(username: String!, password: String!): String
}

type Query{
    getUser(id:ID):User
}

type User {
    id: ID!
    username: String!
    role: Role!
}
enum Role {
    NORMAL
    ADMIN
}
input CredentialsInputDto {
    first_name: String!
    last_name: String!
    nic: String!
    email: String!
    username: String!
    password: String!
    enabled: Boolean!
}