type Mutation {
    createUser(user: CredentialsInputDto): User
    login(email: String!, password: String!): User
}

type User {
    token: String
    user: User
}

input CredentialsInputDto {
    first_name: String!
    last_name: String!
    nic: String!
    nic: String!
    email: String!
    username: String!
    password: String!
    enabled: Boolean!
}